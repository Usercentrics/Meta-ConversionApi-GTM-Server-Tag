// Imports
const getAllEventData = require('getAllEventData');
const getType = require('getType');
const sendHttpRequest = require('sendHttpRequest');
const JSON = require('JSON');
const Math = require('Math');
const getTimestampMillis = require('getTimestampMillis');
const sha256Sync = require('sha256Sync');
const toBase64 = require('toBase64');
const fromBase64 = require('fromBase64');
const getCookieValues = require('getCookieValues');
const setCookie = require('setCookie');
const decodeUriComponent = require('decodeUriComponent');
const parseUrl = require('parseUrl');
const computeEffectiveTldPlusOne = require('computeEffectiveTldPlusOne');


// Constants
const CAPI_ENDPOINT = "https://graph.facebook.com";
const CAPI_VERSION = "v18.0";
const CAPI_PARTNER_AGENT = "usercentrics-gtmss-1.0.0"; 

const EVENT_MAPPING = {
    'add_payment_info': 'AddPaymentInfo',
    'add_to_cart': 'AddToCart',
    'add_to_wishlist': 'AddToWishlist',
    'begin_checkout': 'InitiateCheckout',
    'generate_lead': 'Lead',
    'gtm.dom': 'PageView',
    'page_view': 'PageView',
    'purchase': 'Purchase',
    'search': 'Search',
    'sign_up': 'CompleteRegistration',
    'view_item': 'ViewContent'
};


// Check required parameters
if (!data.pixelId) {
    data.gtmOnFailure();
    return;
}

if (!data.capiAccessToken) {
    data.gtmOnFailure();
    return;
}



function getMetaEventName(gtmEventName) {
return EVENT_MAPPING[gtmEventName] || gtmEventName;
}


function isAlreadyHashed(input){
    return input && (input.match('^[A-Fa-f0-9]{64}$') != null);
}

function hashFunction(input){
    const type = getType(input);
    if(type == 'undefined' || input == 'undefined') {
        return undefined;
    }
    if(input == null || isAlreadyHashed(input)){
        return input;
    }
    return sha256Sync(input.trim().toLowerCase(), {outputEncoding: 'hex'});
}

function getFbcValue() {
    let fbc = gtmEvent['x-fb-ck-fbc'] || getCookieValues('_fbc', true)[0];
    const url = gtmEvent.page_location;
    const subDomainIndex = url ? computeEffectiveTldPlusOne(url).split('.').length - 1 : 1;
    const parsedUrl = parseUrl(url);
    if (parsedUrl && parsedUrl.searchParams.fbclid) {
        fbc = 'fb.' + subDomainIndex + '.' + getTimestampMillis() + '.' + decodeUriComponent(parsedUrl.searchParams.fbclid);
    }  
    return fbc;
}


function setSecureCookie(name, value, expire) {
    setCookie(name, value, {
    domain: 'auto',
    path: '/',
    samesite: 'Lax',
    secure: true,
    'max-age': expire || 7776000, //  90 days
    httpOnly: false
    });
}

function setHttpOnlyCookie(name, value, expire) {
    setCookie(name, value, {
      domain: 'auto',
      path: '/',
      samesite: 'strict',
      secure: true,
      'max-age': expire || 7776000, //  90 days
      httpOnly: true
    });
  }

function tranfsformGtmItemsToCapiContent(items) {
    return items.map(item => {
        return {
            "id": (item.item_id || item.item_name) || undefined,
            "item_price": item.price || undefined,
            "quantity": item.quantity || undefined,
        };
    });
}

function setGtmEecCookie(user_data) {
    let gtmeecCookie = JSON.parse('{}');

    // if user_data has new information, gtmeec data is overriden
    if (user_data.em) {
        gtmeecCookie.em = user_data.em;
    }
    if (user_data.ph) {
        gtmeecCookie.ph = user_data.ph;
    }
    if (user_data.ln) {
        gtmeecCookie.ln = user_data.ln;
    }
    if (user_data.fn) {
        gtmeecCookie.fn = user_data.fn;
    }
    if (user_data.ct) {
        gtmeecCookie.ct = user_data.ct;
    }
    if (user_data.st) {
        gtmeecCookie.st = user_data.st;
    }
    if (user_data.zp) {
        gtmeecCookie.zp = user_data.zp;
    }
    if (user_data.ge) {
        gtmeecCookie.ge = user_data.ge;
    }
    if (user_data.db) {
        gtmeecCookie.db = user_data.db;
    }
    if (user_data.country) {
        gtmeecCookie.country = user_data.country;
    }
    if (user_data.external_id){
        gtmeecCookie.external_id = user_data.external_id;
    }
    if (user_data.fb_login_id) {
        gtmeecCookie.fb_login_id = user_data.fb_login_id;
    }
  
    const cookieJsonStr = JSON.stringify(gtmeecCookie);
    const gtmeecCookieValueBase64 = toBase64(cookieJsonStr);
    setHttpOnlyCookie('_gtmeec', gtmeecCookieValueBase64);
}

function enhanceEventUserData(eventUserData) {

    const cookieValues = getCookieValues('_gtmeec', true);

    if (!cookieValues) {
        return eventUserData;
    }

    if (cookieValues.length == 0) {
        return eventUserData;
    }

    const encodedValue = cookieValues[0];

    if (!encodedValue) {
        return eventUserData;
    }

    const jsonStr = fromBase64(encodedValue);
    if (!jsonStr) {
        return eventUserData;
    }

    const gtmeecData = JSON.parse(jsonStr);

    if (gtmeecData) {
        var properties = [
            'em', 'ph', 'ln', 'fn', 'ct', 'st', 'zp', 'ge', 'db', 'country', 'external_id', 'fb_login_id'
        ];
        for (var i = 0; i < properties.length; i++) {
            var prop = properties[i];
            // eventUserData has precedence over cookie data
            if (!eventUserData[prop] && gtmeecData[prop]) {
                eventUserData[prop] = gtmeecData[prop];
            }
        }
    }

    return eventUserData;
}

//send events to CAPI Server
function sendEventToCapiServers(event) {

   
    let partnerAgent = CAPI_PARTNER_AGENT;
    if (data.enableEventEnhancement) {
      event.user_data = enhanceEventUserData(event.user_data); 
      partnerAgent =  CAPI_PARTNER_AGENT + '-ee';
    }
  
    const eventRequest = {data: [event], partner_agent: partnerAgent};
  
    if(gtmEvent.test_event_code || data.testEventCode) {
      eventRequest.test_event_code = gtmEvent.test_event_code ? gtmEvent.test_event_code : data.testEventCode;
    }
  
    const routeParams = 'events?access_token=' + data.capiAccessToken;
    const graphEndpoint = [CAPI_ENDPOINT,
                           CAPI_VERSION,
                           data.pixelId,
                           routeParams].join('/');
  
    const requestHeaders = {headers: {'content-type': 'application/json'}, method: 'POST'};
    const response = sendHttpRequest(
        graphEndpoint,
        (statusCode, headers, response) => {
            if (statusCode >= 200 && statusCode < 300) {

                if (data.extendCookies && event.user_data.fbc) {
                    setSecureCookie('_fbc', event.user_data.fbc); 
                }

                if (data.extendCookies && event.user_data.fbp) {
                    setSecureCookie('_fbp', event.user_data.fbp);  
                }

                if (data.enableEventEnhancement) {
                    setGtmEecCookie(event.user_data); 
                }

                data.gtmOnSuccess();
            } else {
                data.gtmOnFailure();
            }
        },
        requestHeaders,
        JSON.stringify(eventRequest)
    );
   
    return response;
}
 
  
// Get event data
const gtmEvent = getAllEventData();
const capiEvent = {};
capiEvent.event_name = getMetaEventName(gtmEvent.event_name);
capiEvent.event_time = gtmEvent.event_time || (Math.round(getTimestampMillis() / 1000));
capiEvent.event_id = gtmEvent.event_id;
capiEvent.event_source_url = gtmEvent.page_location;
if(gtmEvent.action_source || data.actionSource) {
    capiEvent.action_source = gtmEvent.action_source ? gtmEvent.action_source : data.actionSource;
}
capiEvent.referrer_url = gtmEvent.page_referrer;



// see https://developers.facebook.com/docs/marketing-api/conversions-api/guides/gtm-server-side/?locale=en_US#sending-meta-browser-id-and-meta-click-id
capiEvent.user_data = {};
const userEmail = gtmEvent['x-fb-ud-em'] || 
    (gtmEvent.user_data != null ? hashFunction(gtmEvent.user_data.email_address) : undefined) || 
    (gtmEvent.user_data != null ? hashFunction(gtmEvent.user_data.email) : undefined);
capiEvent.user_data.em = userEmail;


let normalizedPhoneNumber = null;
if (gtmEvent.user_data && gtmEvent.user_data.phone_number) {
  normalizedPhoneNumber = gtmEvent.user_data.phone_number.replace("+", "").replace("-", "").replace(" ", "").replace("(", "").replace(")", "");
  normalizedPhoneNumber = hashFunction(normalizedPhoneNumber);
}
capiEvent.user_data.ph = gtmEvent['x-fb-ud-ph'] || (normalizedPhoneNumber != null ? normalizedPhoneNumber : undefined);

const userAddress = (gtmEvent.user_data != null && gtmEvent.user_data.address != null) ? gtmEvent.user_data.address : {};
capiEvent.user_data.fn = gtmEvent['x-fb-ud-fn'] || hashFunction(userAddress.first_name);
capiEvent.user_data.ln = gtmEvent['x-fb-ud-ln'] || hashFunction(userAddress.last_name);
capiEvent.user_data.ct = gtmEvent['x-fb-ud-ct'] || hashFunction(userAddress.city);
capiEvent.user_data.st = gtmEvent['x-fb-ud-st'] || hashFunction(userAddress.region);
capiEvent.user_data.zp = gtmEvent['x-fb-ud-zp'] || hashFunction(userAddress.postal_code);
capiEvent.user_data.country = gtmEvent['x-fb-ud-country'] || hashFunction(userAddress.country);

capiEvent.user_data.db =gtmEvent['x-fb-ud-db'];
capiEvent.user_data.ge = gtmEvent['x-fb-ud-ge'];
capiEvent.user_data.external_id = gtmEvent['x-fb-ud-external_id'];

capiEvent.user_data.client_ip_address = gtmEvent.ip_override;
capiEvent.user_data.client_user_agent = gtmEvent.user_agent;

capiEvent.user_data.fbc = getFbcValue();
capiEvent.user_data.fbp = gtmEvent['x-fb-ck-fbp'] || getCookieValues('_fbp', true)[0];
capiEvent.user_data.subscription_id = gtmEvent['x-fb-ud-subscription_id'];
capiEvent.user_data.fb_login_id = gtmEvent['x-fb-ud-fb-login-id'] || (gtmEvent.user_data != null && gtmEvent.user_data.fb_login_id != null ? gtmEvent.user_data.fb_login_id : undefined);

// see https://developers.facebook.com/docs/marketing-api/conversions-api/guides/gtm-server-side/?locale=en_US#custom-data-params-gtm
capiEvent.custom_data = {};
capiEvent.custom_data.currency = gtmEvent.currency;
if(!capiEvent.custom_data.currency) {
    capiEvent.custom_data.currency = 'USD';
}
capiEvent.custom_data.value = gtmEvent.value;
capiEvent.custom_data.search_string = gtmEvent.search_term;
capiEvent.custom_data.order_id = gtmEvent.transaction_id;
capiEvent.custom_data.content_ids = gtmEvent['x-fb-cd-content_ids'];
capiEvent.custom_data.content_type = gtmEvent['x-fb-cd-content_type'];
capiEvent.custom_data.content_name = gtmEvent['x-fb-cd-content_name'];
capiEvent.custom_data.content_category = gtmEvent['x-fb-cd-content_category'];
capiEvent.custom_data.contents = (gtmEvent['x-fb-cd-contents'] != null && gtmEvent['x-fb-cd-contents'].indexOf('[object Object]') == 0 ? null : (typeof(gtmEvent['x-fb-cd-contents']) == "string" ? JSON.parse(gtmEvent['x-fb-cd-contents']) : gtmEvent['x-fb-cd-contents'])) || (gtmEvent.items != null ? tranfsformGtmItemsToCapiContent(gtmEvent.items) : undefined);
capiEvent.custom_data.num_items = gtmEvent['x-fb-cd-num_items'];
capiEvent.custom_data.predicted_ltv = gtmEvent['x-fb-cd-predicted_ltv'];
capiEvent.custom_data.status = gtmEvent['x-fb-cd-status'];
capiEvent.custom_data.delivery_category = gtmEvent['x-fb-cd-delivery_category'];
const customProperties = (gtmEvent.custom_properties != null) ? (gtmEvent.custom_properties.indexOf('[object Object]') == 0 ? null : (typeof(gtmEvent.custom_properties) == "string" ? JSON.parse(gtmEvent.custom_properties) : gtmEvent.custom_properties))  : {};
for (const property in customProperties) {
    capiEvent.custom_data[property] = customProperties[property];
}

capiEvent.data_processing_options = gtmEvent.data_processing_options;
capiEvent.data_processing_options_country = gtmEvent.data_processing_options_country;
capiEvent.data_processing_options_state = gtmEvent.data_processing_options_state;


sendEventToCapiServers(capiEvent);

// data.gtmOnSuccess at the end
data.gtmOnSuccess();


